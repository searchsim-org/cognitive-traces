# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Cognitive Traces API

# CORS Origins (comma-separated, no spaces)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# AI Model API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
ANALYST_MODEL=claude-3-5-sonnet-20241022
CRITIC_MODEL=gpt-4o
JUDGE_MODEL=gpt-4o

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/cognitive_traces

# Redis
REDIS_URL=redis://localhost:6379/0

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# File Upload
MAX_UPLOAD_SIZE=104857600
UPLOAD_DIR=uploads

# Annotation Settings
BATCH_SIZE=50
ACTIVE_LEARNING_THRESHOLD=0.01

